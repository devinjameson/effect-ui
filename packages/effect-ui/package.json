{
  "name": "@foldkit/core",
  "version": "0.1.0-canary.1",
  "description": "Elm-inspired UI framework powered by Effect",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "pnpm run clean && tsc -p tsconfig.build.json",
    "dev": "tsc -w -p tsconfig.json",
    "watch": "vite build --watch & tsc -p tsconfig.build.json --watch",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "peerDependencies": {
    "effect": "^3.16.8"
  },
  "devDependencies": {
    "effect": "^3.16.8",
    "rimraf": "^6.0.1",
    "typescript": "^5.4.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "effect",
    "ui",
    "elm",
    "functional",
    "typescript"
  ],
  "author": "Devin Jameson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devinjameson/foldkit.git",
    "directory": "packages/foldkit"
  }
}
